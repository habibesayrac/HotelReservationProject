@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_HeadPartial")

<body>
    @await Component.InvokeAsync("_SpinnerPartial")

    <div class="container-xxl bg-white p-0">
        <!-- Spinner Start -->
        <!-- Spinner End -->
        <!-- Header Start -->
        @await Component.InvokeAsync("_NavbarPartial")
        <!-- Header End -->
        <!-- Carousel Start -->
        @await Component.InvokeAsync("_SliderPartial")
        <!-- Carousel End -->
        <!-- Booking Start -->
        @await Component.InvokeAsync("_ReservationPartial")
        <!-- Booking End -->
        <!-- About Start -->
        @await Component.InvokeAsync("_AboutUsPartial")

        <!-- About End -->
        <!-- Room Start -->
        @await Component.InvokeAsync("_OurRoomPartial")

        <!-- Room End -->
        <!-- Video Start -->
        @await Component.InvokeAsync("_TrailerPartial")

        <!-- Video Start -->
        <!-- Service Start -->
        @await Component.InvokeAsync("_ServicePartial")

        <!-- Service End -->
        <!-- Testimonial Start -->
        @await Component.InvokeAsync("_TestimonialPartial")

        <!-- Testimonial End -->
        <!-- Team Start -->
        @await Component.InvokeAsync("_TeamPartial")

        <!-- Team End -->
        <!-- Newsletter Start -->
        <div class="container newsletter mt-5 wow fadeIn" data-wow-delay="0.1s">
            <div class="row justify-content-center">
                <div class="col-lg-10 border rounded p-1">
                    <div class="border rounded text-center p-1">
                        <div class="bg-white rounded text-center p-5">
                            <h4 class="mb-4">Subscribe Our <span class="text-primary text-uppercase">Newsletter</span></h4>
                            <div class="position-relative mx-auto" style="max-width: 400px;">
                                <input class="form-control w-100 py-3 ps-4 pe-5" type="text" placeholder="Enter your email">
                                <button type="button" class="btn btn-primary py-2 px-3 position-absolute top-0 end-0 mt-2 me-2">Submit</button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <!-- Newsletter Start -->
        <!-- Footer Start -->
        @await Component.InvokeAsync("_FooterPartial")

        <!-- Footer End -->
        <!-- Back to Top -->
        <a href="#" class="btn btn-lg btn-primary btn-lg-square back-to-top"><i class="bi bi-arrow-up"></i></a>
    </div>




    <!-- JavaScript Libraries -->
    <script src="https://code.jquery.com/jquery-3.4.1.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/hotel-html-template/lib/wow/wow.min.js"></script>
    <script src="~/hotel-html-template/lib/easing/easing.min.js"></script>
    <script src="~/hotel-html-template/lib/waypoints/waypoints.min.js"></script>
    <script src="~/hotel-html-template/lib/counterup/counterup.min.js"></script>
    <script src="~/hotel-html-template/lib/owlcarousel/owl.carousel.min.js"></script>
    <script src="~/hotel-html-template/lib/tempusdominus/js/moment.min.js"></script>
    <script src="~/hotel-html-template/lib/tempusdominus/js/moment-timezone.min.js"></script>
    <script src="~/hotel-html-template/lib/tempusdominus/js/tempusdominus-bootstrap-4.min.js"></script>

    <!-- Template Javascript -->
    <script src="~/hotel-html-template/js/main.js"></script>
</body>

</html>
